{
  "kind": "collectionType",
  "collectionName": "places",
  "info": {
    "name": "places"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "category": {
      "model": "categories"
    },
    "addresses": {
      "model": "addresses"
    }
  }
}
